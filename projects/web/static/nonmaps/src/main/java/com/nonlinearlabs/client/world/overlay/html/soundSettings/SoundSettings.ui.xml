<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' 
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:HTMLPanel>
		<div class="forms-container dialog-container sound-settings">
			<div class="mt1"></div>
			<div class="eins-he fl-j-center">
				<g:Button ui:field="initialize" text="Initialize"></g:Button>
			</div>
			<div class="mt1"></div>
			<div class="linie"></div>

			<div class="eins-he">
				<div class="w40p fl-j-center section-headline">
					<p>Randomize Sound</p>
				</div>
			</div>
			<div class="slider-container">
				<div class="w20p fl-j-center">
					<g:Label ui:field="randomValueDisplayString" styleName="gui-label-1"></g:Label>
				</div>
				<div class="w60p fl-j-center">
					<input type="range" ui:field="randomValueSlider" class="slider-a" min="0" max="1" step="0.001" value="0"></input>
				</div>
				<div class="w20p fl-j-center">
					<g:Button ui:field="applyRandomization" text="Apply"></g:Button>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>